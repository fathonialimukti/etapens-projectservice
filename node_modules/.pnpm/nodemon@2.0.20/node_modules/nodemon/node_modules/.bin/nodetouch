#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/Supporter/Informatika/LJ D4/TA/app/back-end/new project service/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/Supporter/Informatika/LJ D4/TA/app/back-end/new project service/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../touch@3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../../../../../touch@3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
fi
